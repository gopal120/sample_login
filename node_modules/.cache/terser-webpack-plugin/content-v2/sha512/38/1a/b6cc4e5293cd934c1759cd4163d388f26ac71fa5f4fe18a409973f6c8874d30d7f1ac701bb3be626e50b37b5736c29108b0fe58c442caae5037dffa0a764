ify(data),\n        }\n      );\n    };\n    HttpRequest();\n  }, [data]);\n  return (\n    <Fragment>\n      {!isvalid && (\n        <Layout>\n          <Intro setform={setform} />\n          {!form ? (\n            <Signup data={userinput} setisvalid={setisvalid} />\n          ) : (\n            <Login setisvalid={setisvalid} />\n          )}\n        </Layout>\n      )}\n      {isvalid && <Welcome setisvalid={setisvalid}/>}\n    </Fragment>\n  );\n}\n\nexport default App;\n","import ReactDOM from \"react-dom\";\n\nimport \"./index.css\";\nimport App from \"./App\";\nReactDOM.render(\n    <App />,\n  document.getElementById(\"root\")\n);\n"]}}